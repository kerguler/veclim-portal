{
  "extends": ["react-app"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "no-unused-vars": 1,
    "no-empty": "off",
    "indent": "off",
    "no-mixed-spaces-and-tabs": "off",
    "react/no-unescaped-entities": "off",
    "react/no-children-prop": "off",
    "no-self-assign": "off"
  }
}
